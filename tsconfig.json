{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "config/*": ["src/_shared/config/*"],
      "domain": ["./src/_shared/domain"],
      "domain/*": ["./src/_shared/domain/*"],
      "app": ["./src/_shared/app"],
      "app/*": ["./src/_shared/app/*"],
      "adapter": ["./src/_shared/adapter"],
      "adapter/*": ["./src/_shared/adapter/*"],
      "framework": ["./src/_shared/framework"],
      "framework/*": ["./src/_shared/framework/*"],
      "util": ["./src/_shared/util"],
      "util/*": ["./src/_shared/util/*"],
      "auth": ["./src/auth"],
      "auth/*": ["./src/auth/*"],
      "manager": ["./src/manager"],
      "manager/*": ["./src/manager/*"],
      "adherent": ["./src/adherent"],
      "adherent/*": ["./src/adherent/*"],
      "equipment": ["./src/equipment"],
      "equipment/*": ["./src/equipment/*"],
      "salle": ["./src/salle"],
      "salle/*": ["./src/salle/*"],
      "abonnement": ["./src/abonnement"],
      "abonnement/*": ["./src/abonnement/*"],
      "entraineur": ["./src/entraineur"],
      "entraineur/*": ["./src/entraineur/*"],
      "horaire": ["./src/horaire"],
      "horaire/*": ["./src/horaire/*"],
      "statistic": ["./src/statistic"],
      "statistic/*": ["./src/statistic/*"]
    }
  }
}
